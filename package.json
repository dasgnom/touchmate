{
  "name": "touchmate",
  "description": "A frontend for the space market api",
  "version": "1.0.0",
  "author": "telegnom <max@telegnom.org>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "cleanup": "rimraf dist/*",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:perf": "npm run cleanup && NODE_ENV=production webpack --profile --json > ./webpack-stats.json && webpack-bundle-analyzer ./webpack-stats.json dist/ --mode static -s gzip --report ./BundleReport.html --no-open"
  },
  "dependencies": {
    "bootstrap": "^4.1.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "jquery": "^3.3.1",
    "md5": "^2.2.1",
    "mdi": "^2.2.43",
    "moment": "2.22.1",
    "moment-timezone": "^0.5.16",
    "vue": "^2.5.11",
    "vue-config": "^1.0.0",
    "vue-notification": "^1.3.10",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vuejs-datepicker": "^1.3.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "bootswatch": "^4.0.0",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-joblift": "^1.3.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "eslint-plugin-vue": "^4.5.0",
    "file-loader": "^1.1.4",
    "popper.js": "^1.14.3",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.11.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  }
